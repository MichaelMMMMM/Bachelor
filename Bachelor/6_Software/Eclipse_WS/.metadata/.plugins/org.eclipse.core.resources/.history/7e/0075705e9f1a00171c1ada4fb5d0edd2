#include "CSensorCalibration.h"
#include <iostream>

void CSensorCalibrationAction::entrySensorCalibration()
{
	sTimerTask.setPeriodMS(20);
	sTimerTask.resume(true);
	std::cout << "[*] Control-Comp: Entering Sensor-Calibration . . . " << std::endl;
}
