C1DCompFilter::C1DCompFilter() : mFirstRun(true)
{

}
const C1DCompFilter::OutputType& C1DCompFilter::calcOutput(const C1DCompFilter::InputType& input)
{
	if(mFirstRun == true)
	{
		mFirstRun = false;
		mOutput = input;
	}
	else
	{

	}
	return mOutput;
}
