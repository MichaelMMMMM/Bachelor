CMPUCalib::CMPUCalib(const std::string& calib_file) : mSMatrix{0.0F},
													  mOffsetVector{0.0F},
													  mAcceleration{0.0F}
{

}
void CMPUCalib::calibrate(const CMPUData& data)
{
	Float32 x = static_cast<data.mA_x>;
	Float32 y = static_cast<data.mA_y>;
	Float32 y = static_cast<data.mA_z>;
}
