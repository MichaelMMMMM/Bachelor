/**
 * @file	CTorqueData.h
 * @author	Michael Meindl
 * @date	08.02.2017
 * @brief	Data-Abstraction for the Motor-Torques.
 */
#ifndef CTORQUEDATA_H
#define CTORQUEDATA_H
#include "Global.h"

class CTorqueData
{
public:
	void display();
public:
	CTorqueData();
	CTorqueData(const CTorqueData&) = default;
	CTorqueData& operator=(const CTorqueData&) = default;
	~CTorqueData() = default;
public:
	Float32 mTime;
	Float32 mTorque1;
	Float32 mTorque2;
	Float32 mTorque3;
};

#endif
