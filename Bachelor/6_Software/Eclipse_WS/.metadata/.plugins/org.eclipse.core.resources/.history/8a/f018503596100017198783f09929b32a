#include <iostream>
#include "TMatrix.h"
using namespace std;


int main()
{
	TMatrix<3U, 3U> test1;
	TMatrix<7U, 3U> test2("Debug/Ld_mat.csv");
	test2.display();

	test1[1][1] = 3.0F;
	test1[2][1] = 4.0F;
	test1[3][1] = 5.0F;
	test1[1][2] = 6.0F;
	test1[2][2] = 7.0F;
	test1[3][2] = 8.0F;
	test1[1][3] = 9.0F;
	test1[2][3] = 10.0F;
	test1[3][3] = 11.0F;
	test1.display();

	auto test3 = test2*test1;
	test3.display();
	return 0;
}
