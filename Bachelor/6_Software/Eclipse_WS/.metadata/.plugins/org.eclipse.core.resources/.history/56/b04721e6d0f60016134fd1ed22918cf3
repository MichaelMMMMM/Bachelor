#include "CADCSystem.h"

CADCSystem::CADCSystem() : mSwitch("49", "48", "7", "26"), mADCFD(-1)
{

}
bool CADCSystem::fetchValues(CADCData& data)
{
	mSwitch.selectX0();
	mSwitch.selectX1();
	mSwitch.selectX2();
	mSwitch.selectX3();
	return true;
}
