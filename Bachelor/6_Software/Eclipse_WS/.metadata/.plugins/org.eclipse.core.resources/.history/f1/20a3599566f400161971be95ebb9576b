 * @brief	Method definitions for CLQRTest.
 */
#include "CLQRTest.h"

bool CLQRTest::tryEntry(CMessage& msg, AState*& statePtr)
{
	EEvent event = msg.getEvent();
	if(EEvent::RUN_CONTROL == event)
	{
		statePtr = this;
		return true;
	}
	return false;
}
