var testHandler;// = new CTestHandler();
var webSocket;//   = new CWebSocket(testHandler);

function onLoadJS()
{
	testHandler = new CTestHandler();
	webSocket   = new CWebSocket(testHandler);
}
function onConnectClicked()
{
	webSocket.init();
	alert("test");
}
function onDisconnectClicked()
{
	webSocket.close();
}