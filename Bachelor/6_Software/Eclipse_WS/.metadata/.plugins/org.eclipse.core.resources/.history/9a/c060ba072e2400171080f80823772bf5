void C3DStateEstimate::estimateUR(const C3DStateEstimate::InputType& input)
{
	for(UInt32 n = 0U; n < 3U; n++)
	{
		mOutput[7U+n][1] = input.mADC[n] + mOutput[4U+n];
	}
}
