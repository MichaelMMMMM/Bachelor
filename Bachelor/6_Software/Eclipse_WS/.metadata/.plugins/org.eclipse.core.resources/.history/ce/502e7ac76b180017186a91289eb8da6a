var d1 = [];
var d2 = [];
var d3 = [];
var time = 0;

myVar = setInterval(myTimer, 20);

var myPlot;

function myTimer()
{
	time = time + 0.02;
	data = [time, Math.sin(time), Math.cos(time), 2*Math.sin(time)];
	myPlot.addData(data);
}

function drawData()
{
	$.plot("#placeholder", [d1, d2, d3]);
}

function onLoadJS()
{
	myPlot = new CPlot("placeholder", 3, ["plot1", "plot2", "plot3"]);
	//$.plot("#placeholder", [d1, d2, d3]);
	//$("#footer").prepend("Float" + $.plot.version + "$ndash;");
	
}
