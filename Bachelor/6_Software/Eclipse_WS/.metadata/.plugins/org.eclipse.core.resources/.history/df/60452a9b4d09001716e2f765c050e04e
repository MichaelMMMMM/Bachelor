#include "C1DOffsetCorrection.h"

C1DOffsetCorrection::C1DOffsetCorrection() : mActiveFlag(false)
{
	mOffset.scalarAt(1U, 1U) = sX1Offset;
	mOffset.scalarAt(2U, 1U) = sX2Offset;
	mOffset.scalarAt(3U, 1U) = sX3Offset;
}

