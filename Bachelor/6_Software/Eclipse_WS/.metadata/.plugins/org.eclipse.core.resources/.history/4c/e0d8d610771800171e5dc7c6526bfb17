function CTestHandler()
{
	this.mTestPlot = new CPlot("placeholder", 2, ["phi1", "phi2"]);
};
CTestHandler.prototype.handleMessage = function(evt)
{
	if(evt.data === 'string')
	{
		var json = JSON.parse(evt.data);
		alert(json.time);
		this.mTestPlot.addData([json.time, json.phi1, json.phi2]);	
	}

};